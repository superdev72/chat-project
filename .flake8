[flake8]
max-line-length = 100
extend-ignore = E203, W503, E501
exclude = 
    .git,
    __pycache__,
    migrations,
    .venv,
    venv,
    .tox,
    .eggs,
    *.egg,
    build,
    dist,
    .mypy_cache,
    .pytest_cache
max-complexity = 10
per-file-ignores =
    __init__.py:F401
    */migrations/*:E501
